<h1 class="mt-5">MONOPOLY</h1>
<form action="/cadastro" method="post">
  <div class="mt-3">
    <input type="text" class="form-control p-2" name="nome" placeholder="Nome">
  </div>
  <div class="mt-3">
    <input type="text" class="form-control p-2" name="email" placeholder="Email">
  </div>
  <div class="mt-3">
    <input type="password" class="form-control p-2" name="senha" placeholder="Senha">
  </div>
  <div class="mt-3">
    <input type="submit" class="btn btn-dark " name="cadastro" placeholder="Cadastrar">
  </div>
</form>
